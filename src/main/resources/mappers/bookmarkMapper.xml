<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace=" ">

	 <insert id="insertBookmark" parameterType="BookMarkDTO">
	 	insert into mb_bookmark
	 	values(#{mb_id}, #{de_licencenum})
	 </insert>

	<select id="selectBookMark"  parameterType="BookMarkDTO" resultType="BookMarkDTO">
		select * from mb_bookmark
		where mb_id = #{mb_id}
	</select>
	
</mapper>