<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.exam.MemberMapper">
	<select id="getTime" resultType="String">
		select now()
	</select>
	
	<insert id="insertMember"><!-- parmType넣지않은 이유는 mybatisConfig.xml 참조  -->
		insert into tbl_member (uid, pwd, username, email)
		values (#{uid}, #{pwd}, #{username}, #{email})
	</insert>
</mapper>